@model List<Category>
   <h1>Category Controller</h1>
   <table class="table table-bordered table-striped">
       <thead>
       <th>Category Name</th>
  
       <th>Category Decscription</th>
       
        </thead>
        <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.Description
                </td>
            </tr>
            }
        </tbody>
   </table>